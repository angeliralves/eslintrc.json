{
  "name": "eslintrc.json",
  "version": "0.5.5",
  "description": "Quickly create an opinionated ESLint configuration",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "bin": "bin/index.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "unbuild",
    "release": "pnpm build && bumpp -y && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "eslint",
    "eslintrc",
    "eslint-config",
    "prettier",
    "prettierrc",
    "prettier-config",
    "prettier-eslint"
  ],
  "author": "Peng Sha <jack@shapeng1998.com>",
  "license": "MIT",
  "devDependencies": {
    "@shapeng1998/eslint-config": "^0.5.10",
    "@types/eslint": "^8.4.3",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "prettier": "^2.7.1",
    "tsx": "^3.6.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4"
  },
  "dependencies": {
    "@antfu/ni": "^0.16.3"
  }
}
